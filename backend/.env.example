# Django Core Settings
# Replace with a new, securely generated secret key for your environment.
DJANGO_SECRET_KEY=your_django_secret_key_here
DEBUG=True

# Database Configuration
# These should match your local MySQL setup.
DB_NAME=applaude_db
DB_USER=root
DB_PASSWORD=your_mysql_password
DB_HOST=localhost
DB_PORT=3306

# Google Gemini API
# Your API key for Google's Gemini services.
GEMINI_API_KEY=your_gemini_api_key_here

# Paystack Payment Gateway
# Your secret key from your Paystack account.
PAYSTACK_SECRET_KEY=your_paystack_secret_key_here
